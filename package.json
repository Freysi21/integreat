{
  "name": "integreat",
  "version": "0.0.2",
  "description": "Node.js integration layer",
  "author": "Kjell-Morten Bratsberg Thorsen <post@kjellmorten.no> (http://kjellmorten.no/)",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "NODE_ENV=test nyc ava",
    "test:watch": "NODE_ENV=test ava --watch",
    "lint": "standard"
  },
  "ava": {
    "files": [
      "lib/**/*-test.js"
    ],
    "source": [
      "lib/**/!(*-test).js"
    ],
    "babel": {
      "presets": [
        "babel-preset-node7"
      ]
    }
  },
  "nyc": {
    "exclude": [
      "**/*-test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjellmorten/integreat.git"
  },
  "bugs": {
    "url": "https://github.com/kjellmorten/integreat/issues"
  },
  "homepage": "https://github.com/kjellmorten/integreat#readme",
  "engines": {
    "node": "6.9.x",
    "npm": ">= 3.10.9"
  },
  "dependencies": {
    "dbdb": "^1.4.3",
    "debug": "^2.6.0",
    "dot-prop": "^4.1.0",
    "request": "^2.79.0",
    "rx": "^4.1.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^0.18.0",
    "babel-preset-node7": "^1.4.0",
    "dbdbmock": "^1.4.3",
    "nock": "^9.0.2",
    "nyc": "^10.0.0",
    "sinon": "^1.17.6",
    "standard": "^8.6.0"
  }
}
